<form method="POST" action="fillCourse?_method=PUT" class="mt-5">
   {{!-- <input type="hidden" name="_METHOD" value="PUT"/> --}}
  <div class="form-group">
    <label for="title">title</label>
    <input type="text" class="form-control" id="title" name="title" value="{{course.title}}">
  </div>
  <div class="form-group">
    <label for="description">description</label>
    <input type="text" class="form-control" id="description" name="description" value="{{course.description}}" >
  </div>
  <div class="form-group">
    <label for="Level">Level</label>
    <input type="text" class="form-control" id="Level" name="level" value="{{course.level}}"  >
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>
  <div class="form-group">
    <label for="videoID">videoID</label>
    <input type="text" class="form-control" id="videoID" name="videoID" value="{{course.videoID}}" >
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>